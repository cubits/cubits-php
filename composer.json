{
    "name": "cubits/apiclient",
    "description": "A client library for the Cubits API on https://cubits.com",
    "keywords": ["cubits", "bitcoin", "api", "client"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hannes Schulz",
            "email": "hannes.schulz@cubits.com"
        }
    ],
    "require": {
        "php": ">=5.6|7.0",
        "psr/log": "~1",
        "guzzlehttp/guzzle": "^6.2",
        "guzzlehttp/psr7": "^1.3"
    },
    "autoload": {
        "psr-4": { "Cubits\\ApiClient\\": "src" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }
}
